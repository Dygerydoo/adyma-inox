    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-55530290-1', 'auto');
      ga('send', 'pageview');
    </script>
    <script>
    $(document).ready(function () {
        $.cookieCuttr({
        cookieAnalytics: true,
        cookieWhatAreTheyLink: 'http://www.allaboutcookies.org/es/',
        cookieAnalyticsMessage: "Hacemos uso de cookies, para monitorizar las visitas a nuestra web, no guardamos ningún tipo de información personal.",
        });
    });
    </script>
    {% block javascripts %}{% endblock %}
    </body>
</html>
