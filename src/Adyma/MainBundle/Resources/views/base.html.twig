                <a href="{{ path('contacto') }}">{{ 'menu.contacto'|trans }}</a>
            </li>
        </ul>
    </nav>
    {% block body %}{% endblock %}
    </div>
    <footer>
        <p> {{ 'footer.direction'|trans }} | <a class="infl" href="#">{{ 'footer.legal'|trans }}</a></p>
        <div class="socialfoo">
            <a href=""><img src={{ asset('bundles/adymamain/images/social/fb.png') }} alt="Facebook"></a>
